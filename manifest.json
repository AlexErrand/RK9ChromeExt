{
  "name": "RK9 Showdown Converter",
  "version": "1.0",
  "description": "A Chrome Extension",
  "manifest_version": 3,
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["https://rk9.gg/*"],
  "content_scripts": [
    {
      "matches": ["https://rk9.gg/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "48": "masterrankribbon.png"
  }
}
